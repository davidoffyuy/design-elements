{
  "name": "designelements",
  "version": "1.0.0",
  "description": "David's personal design playground",
  "main": "public/index.html",
  "scripts": {
    "build": "npm run hbs && npm run scss",
    "hbs": "handlebars -m templates/ -f public/js/templates/templates.js",
    "scss": "npm run scss:fading-slider-class && npm run scss:scroll-shadow",
    "scss:fading-slider-class": "sass --no-source-map scss/fading-slider-class.scss public/stylesheets/compiled/fading-slider-class.css",
    "scss:scroll-shadow": "sass --no-source-map scss/scroll-shadow.scss public/stylesheets/compiled/scroll-shadow.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidoffyuy/design-elements.git"
  },
  "author": "David Yu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/davidoffyuy/design-elements/issues"
  },
  "homepage": "https://github.com/davidoffyuy/design-elements#readme",
  "dependencies": {
    "handlebars": "^4.7.6",
    "sass": "^1.26.10"
  }
}
